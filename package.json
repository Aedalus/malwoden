{
  "name": "malwoden",
  "version": "0.1.2",
  "description": "",
  "keywords": [
    "rogue",
    "roguelike",
    "typescript",
    "cp437",
    "ascii",
    "generation"
  ],
  "main": "dist/malwoden.umd.js",
  "module": "dist/malwoden.es5.js",
  "typings": "dist/types/malwoden.d.ts",
  "files": [
    "dist"
  ],
  "author": " <TheAlexanderHiggins@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aedalus/malwoden"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --mode modules src --readme none --excludePrivate --excludeProtected --excludeExternals",
    "start": "rollup -c rollup.config.ts -w",
    "test_ci": "jest --coverage src --runInBand",
    "test": "jest --coverage src && jest-coverage-badges",
    "test:watch": "jest --coverage --watch src",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "changelog": "conventional-changelog -i CHANGELOG.md -s"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": {
          "target": "es6"
        }
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}",
      "src/**.{js,ts}",
      "src/**/*.{js,ts}"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^23.3.2",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^10.11.0",
    "colors": "^1.3.2",
    "commitizen": "^4.2.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.1",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "jsdom": "^16.4.0",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "madge": "^3.4.4",
    "prettier": "^2.2.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "semantic-release": "^17.2.3",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^26.4.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.19.2",
    "typedoc-plugin-external-module-name": "^4.0.5",
    "typedoc-plugin-remove-references": "0.0.5",
    "typescript": "^4.1.2"
  },
  "dependencies": {}
}
